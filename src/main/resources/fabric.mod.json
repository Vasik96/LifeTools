      "com.lifetools.Fly",
      "com.lifetools.Teleport",
      "com.lifetools.Speed",
      "com.lifetools.LifeTools",
      "com.lifetools.NoFall",
      "com.lifetools.BoatFly",
      "com.lifetools.Scaffold",
      "com.lifetools.ClientEffects"

